{
  "compilerOptions": {
    "target": "ES2016", /* 将TS编译成目标版本js 'ES3' (默认), 'ES5', 'ES2015', 'ES2016'*/
    "module": "commonjs", 
    "outDir": "./dist/",  /* 编译输出文件夹 */
    "rootDir": "./src/", /* 项目源码文件夹 */
    "strict": true, 
    "esModuleInterop": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
  }
}